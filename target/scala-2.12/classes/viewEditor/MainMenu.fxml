<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainPage" prefHeight="400.0" prefWidth="600.0" stylesheets="@background.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Game.Starter.MainMenuController">
   <children>
      <Label fx:id="menuTitle" layoutX="207.0" layoutY="14.0" text="PAC MAN" textAlignment="CENTER">
         <font>
            <Font name="Rockwell Extra Bold" size="40.0" />
         </font></Label>
      <VBox fx:id="btnBox" alignment="CENTER" layoutX="219.0" layoutY="245.0" prefHeight="141.0" prefWidth="164.0" AnchorPane.topAnchor="245.0">
         <children>
            <Button fx:id="btnGame" mnemonicParsing="false" onAction="#StartGame" prefHeight="35.0" prefWidth="150.0" text="Start Game">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button fx:id="btnBoard" mnemonicParsing="false" onAction="#leaderBoard" prefHeight="35.0" prefWidth="150.0" text="LeaderBoard">
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btnQuit" mnemonicParsing="false" onAction="#quit" prefHeight="35.0" prefWidth="150.0" text="Quit">
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font></Button>
         </children>
      </VBox>
      <TextField fx:id="playerName" layoutX="235.0" layoutY="85.0" prefHeight="15.0" prefWidth="133.0" text="Enter Name">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
